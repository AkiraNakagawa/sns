	<h1>Login Page</h1>
	<h4>Please put your email and password for login to your account.</h4>
	<br>
	<form role="form" method="post" action="/sessions/create">
    	<div class="form-group">
      		<label for="email">Email:</label>
      		<input type="email" class="form-control" name="email" id="email" placeholder="Enter email" required="true">
    	</div>
    	<div class="form-group">
      		<label for="pwd">Password:</label>
      		<input type="password" class="form-control" name="pwd" id="pwd" placeholder="Enter password" required="true">
    	</div>
    	
    	<input name="Login" type="submit" id="Login" value="Login" class="btn btn-default" />
  	</form>


</div>
