<h2>User Information</h2>
<table class="table">
  <tbody>
    <tr>
      <td>Name: </td>
      <td><%= @userinfo.firstname %>  <%= @userinfo.lastname %></td>
    </tr>
    <tr>
      <td>School: </td>
      <td><%= @userinfo.school %></td>
    </tr>
    <tr>
      <td>Email Address: </td>
